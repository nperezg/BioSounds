<div class="form-group mr-2">
    <label class="col-form-label-sm mr-2" for="x">Time:</label>
    <input type="text" class="form-control form-control-sm" id="x" name="t_min" size="5" value="{{ minTime }}" title="Minimum time">
</div>
<div class="form-group mr-2">
    <label class="col-form-label-sm mr-2" for="w">to</label>
    <input type="text" class="form-control form-control-sm" id="w" name="t_max" size="5" value="{{ maxTime }}" title="Maximum time">
    <label class="col-form-label-sm mr-2" for="w">s</label>
</div>
<div class="form-group mr-2">
    <label class="col-form-label-sm mr-2" for="y">Frequency:</label>
    <input type="text" class="form-control form-control-sm" id="y" name="f_min" size="4" value="{{ minFrequency }}" title="Minimum frequency">
</div>
<div class="form-group">
    <label class="col-form-label-sm mr-2" for="h">to</label>
    <input type="text" class="form-control form-control-sm" id="h" name="f_max" size="4" value="{{ maxFrequency }}" title="Maximum frequency">
    <label class="col-form-label-sm mr-2" for="h">Hz</label>
</div>
